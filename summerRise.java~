import java.util.*;
import java.io.*;

public class summerRise extends Scanner {
    private String text;

    public static void main (String[]args) {
	String filename = args[0];
	Scanner scan = new Scanner(new File(filename));
	while (scan.hasNext()) {
	    text.concat(next());
	}
	System.out.println(text);
    }
}
